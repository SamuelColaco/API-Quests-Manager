
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum roles {
  admin
  member
}

model Users{
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  role roles @default(member)
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("Users")
}
